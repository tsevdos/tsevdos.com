{
  "name": "astro-tsevdoscom",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "NODE_OPTIONS=--max_old_space_size=4096 npm run check && astro build && cp ./_redirects ./dist",
    "check": "npm run lint && npm run prettier && NODE_OPTIONS=--max_old_space_size=4096 astro check",
    "preview": "astro preview",
